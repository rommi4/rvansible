[user]
  name = Roman Verenich
  email = r_verenich@wargaming.net
[core]
  editor = vim
[alias]
  ci = commit
  co = checkout
  st = status
  br = branch
  hist = log --pretty=format:"%Cred%h %Creset%ad %Cred| [%an] | %Cgreen%s%d" --graph --date=short --color""
  dc = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset" --abbrev-commit --date=relative""
  gr = log --graph --oneline
[color]
  ui = auto
[push]
  default = simple
[credential]
	helper = cache
